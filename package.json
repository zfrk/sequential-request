{
  "name": "seqreq",
  "version": "0.0.6",
  "description": "Sequential requests using simple json/yaml structures",
  "main": "index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "seqreq": "./lib/bin/seqreq.js"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "postpublish": "rimraf ./lib",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build && husky install",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "test": "jest --config jestconfig.json",
    "test:watch": "jest --config jestconfig.json --watch",
    "test:yaml": "seqreq tmp/npm_test/*.yml",
    "version": "npm run format && git add -A src"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/packagely/sequential-request.git"
  },
  "keywords": [
    "sequential",
    "request",
    "testing",
    "restful",
    "json"
  ],
  "author": "Emre INEL",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/packagely/sequential-request/issues"
  },
  "homepage": "https://github.com/packagely/sequential-request#readme",
  "devDependencies": {
    "@types/clear": "^0.1.2",
    "@types/jest": "^27.0.1",
    "@types/js-yaml": "^4.0.3",
    "@types/node-fetch": "^2.5.12",
    "@types/ramda": "^0.27.44",
    "@types/set-cookie-parser": "^2.4.1",
    "husky": "^7.0.2",
    "jest": "^27.1.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "clear": "^0.1.0",
    "js-yaml": "^4.1.0",
    "jsonata": "^1.8.5",
    "node-fetch": "^2.6.1",
    "ramda": "^0.27.1",
    "set-cookie-parser": "^2.4.8",
    "yargs": "^17.1.1"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
